From 3e235b5e88cc860e5aced7cc989b44d51a5237ea Mon Sep 17 00:00:00 2001
From: Shilin Victor <chrono.monochrome@gmail.com>
Date: Sat, 18 Jul 2015 12:27:16 +0300
Subject: [PATCH] 0004-Change-optimizations-flags.patch

Change-Id: I0b88c878b9b3a1325c6b85937b12fdc1dae8d888
---
 core/combo/TARGET_linux-arm.mk | 11 ++++++++---
 1 file changed, 8 insertions(+), 3 deletions(-)

diff --git a/core/combo/TARGET_linux-arm.mk b/core/combo/TARGET_linux-arm.mk
index 3497662..65b9f9f 100644
--- a/core/combo/TARGET_linux-arm.mk
+++ b/core/combo/TARGET_linux-arm.mk
@@ -70,14 +70,19 @@ TARGET_NO_UNDEFINED_LDFLAGS := -Wl,--no-undefined
 
 TARGET_arm_CFLAGS :=    -O2 \
                         -fomit-frame-pointer \
-                        -fstrict-aliasing    \
-                        -funswitch-loops
+                        -fno-strict-aliasing    \
+                        -funswitch-loops \
+			-Wno-error=unused-parameter \
+                        -Wno-error=unused-variable 
 
 # Modules can choose to compile some source as thumb.
 TARGET_thumb_CFLAGS :=  -mthumb \
                         -Os \
                         -fomit-frame-pointer \
-                        -fno-strict-aliasing
+			-Wno-error=unused-parameter \
+			-Wno-error=unused-variable \
+                        -fno-strict-aliasing 
+
 
 # Set FORCE_ARM_DEBUGGING to "true" in your buildspec.mk
 # or in your environment to force a full arm build, even for
