From 9c0b85ce93894b8a55946c3dd9a0f629a9364ccb Mon Sep 17 00:00:00 2001
From: Shilin Victor <chrono.monochrome@gmail.com>
Date: Thu, 16 Jul 2015 09:13:02 +0300
Subject: [PATCH] Remove strict aliasing flag

Change-Id: Ia90b68dc615c7b14095266ee2ceef8bdc99afc8e
---
 core/combo/TARGET_linux-arm.mk | 6 ------
 1 file changed, 6 deletions(-)

diff --git a/core/combo/TARGET_linux-arm.mk b/core/combo/TARGET_linux-arm.mk
index 9c20e7c..49c8041 100755
--- a/core/combo/TARGET_linux-arm.mk
+++ b/core/combo/TARGET_linux-arm.mk
@@ -48,14 +48,8 @@ ifeq ($(strip $(wildcard $(TARGET_ARCH_SPECIFIC_MAKEFILE))),)
 $(error Unknown ARM architecture version: $(TARGET_ARCH_VARIANT))
 endif
 
-ifeq ($(strip $(DONT_WARN_STRICT_ALIASING)),)
-STRICT_ALIASING_WARNINGS := \
-                        -Wstrict-aliasing=2 \
-                        -Werror=strict-aliasing
-else
 STRICT_ALIASING_WARNINGS := \
                         -Wno-strict-aliasing
-endif
 
 ifeq ($(strip $(BONE_STOCK)),)
 TARGET_ARM_O := 3
-- 
