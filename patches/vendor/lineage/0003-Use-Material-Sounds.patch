From e0071b8499931bf917c595bac6ab36cec2108b11 Mon Sep 17 00:00:00 2001
From: Shilin Victor <chrono.monochrome@gmail.com>
Date: Mon, 20 Aug 2018 22:57:43 +0300
Subject: [PATCH 03/11] Use Material Sounds

---
 config/common.mk    | 4 ++--
 config/telephony.mk | 2 +-
 2 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/config/common.mk b/config/common.mk
index a5b35320..219bd3ad 100644
--- a/config/common.mk
+++ b/config/common.mk
@@ -18,8 +18,8 @@ PRODUCT_SYSTEM_DEFAULT_PROPERTIES += \
 
 # Default notification/alarm sounds
 PRODUCT_SYSTEM_DEFAULT_PROPERTIES += \
-    ro.config.notification_sound=Argon.ogg \
-    ro.config.alarm_alert=Hassium.ogg
+    ro.config.notification_sound=Ceres.ogg \
+    ro.config.alarm_alert=Carbon.ogg
 
 ifneq ($(TARGET_BUILD_VARIANT),user)
 # Thank you, please drive thru!
diff --git a/config/telephony.mk b/config/telephony.mk
index 5f23cc48..211f37b8 100644
--- a/config/telephony.mk
+++ b/config/telephony.mk
@@ -14,7 +14,7 @@ PRODUCT_PACKAGES += \
 
 # Default ringtone
 PRODUCT_SYSTEM_DEFAULT_PROPERTIES += \
-    ro.config.ringtone=Orion.ogg
+    ro.config.ringtone=Dione.ogg
 
 # Tethering - allow without requiring a provisioning app
 # (for devices that check this)
-- 
