From fe8472da066d14fdb4711d87b0f1a5d6fb3ff418 Mon Sep 17 00:00:00 2001
From: Lorenzo Colitti <lorenzo@google.com>
Date: Mon, 1 Aug 2016 18:14:31 +0900
Subject: [PATCH] Don't start tethering if IPv6 RPF is not supported.

---
 server/NatController.cpp | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/server/NatController.cpp b/server/NatController.cpp
index 58c732d..45b64b8 100644
--- a/server/NatController.cpp
+++ b/server/NatController.cpp
@@ -205,7 +205,7 @@ int NatController::setForwardRules(bool add, const char *intIface, const char *e
         "*raw\n"
         "%s %s -i %s -m rpfilter --invert ! -s fe80::/64 -j DROP\n"
         "COMMIT\n", op, LOCAL_RAW_PREROUTING, intIface);
-    if (iptablesRestoreFunction(V6, rpfilterCmd) == -1 && add) {
+    if (iptablesRestoreFunction(V6, rpfilterCmd) == -1 && add && false /* STOPSHIP */) {
         return -1;
     }
 
