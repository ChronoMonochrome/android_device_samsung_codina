From 3e7227c99f9aed259d23994787ff56ef3fefa82f Mon Sep 17 00:00:00 2001
From: Dan Pasanen <invisiblek@cyanogenmod.org>
Date: Thu, 10 Nov 2016 21:24:16 -0600
Subject: [PATCH] [1/2] init.rc: set DOWNLOAD_CACHE based on prop

diff --git a/rootdir/init.rc b/rootdir/init.rc
index ec9ba99..daede42 100644
--- a/rootdir/init.rc
+++ b/rootdir/init.rc
@@ -237,7 +237,7 @@
     # expecting it to point to /proc/self/fd
     symlink /proc/self/fd /dev/fd
 
-    export DOWNLOAD_CACHE /data/cache
+    export DOWNLOAD_CACHE /cache
 
     # set RLIMIT_NICE to allow priorities from 19 to -20
     setrlimit 13 40 40
